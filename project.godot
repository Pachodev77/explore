; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=4

_global_script_classes=[ {
"base": "KinematicBody",
"class": "Horse",
"language": "GDScript",
"path": "res://ui/scripts/Horse.gd"
} ]
_global_script_class_icons={
"Horse": ""
}

[application]

config/name="Juego1"
run/main_scene="res://ui/scenes/MainMenu.tscn"
config/icon="res://ui/icons/inventory.jpg"

[autoload]

InventoryManager="*res://ui/scripts/InventoryManager.gd"
LoadingManager="*res://ui/scripts/LoadingManager.gd"
SceneManager="*res://ui/scripts/SceneManager.gd"

[display]

window/size/width=1280
window/size/height=720
window/size/test_width=1280
window/size/test_height=720
window/stretch/mode="2d"
window/stretch/aspect="expand"

[gui]

common/drop_mouse_on_gui_input_disabled=true

[physics]

common/enable_pause_aware_picking=true

[rendering]

quality/driver/driver_name="GLES2"
vram_compression/import_etc=true
vram_compression/import_etc2=false
quality/shadows/filter_mode=0
environment/default_environment="res://default_env.tres"
